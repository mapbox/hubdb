{
  "name": "hubdb",
  "version": "1.1.1",
  "description": "a github-powered database",
  "main": "index.js",
  "scripts": {
    "test": "jshint index.js",
    "doc": "dox -r < index.js | doxme --readme > README.md",
    "prepublish": "browserify index.js -s Hubdb > hubdb.js"
  },
  "browserify": {
    "transform": [
      "envify"
    ]
  },
  "keywords": [
    "github",
    "db",
    "database",
    "nosql"
  ],
  "author": "Tom MacWright",
  "license": "ISC",
  "dependencies": {
    "atob": "^1.1.2",
    "btoa": "^1.1.2",
    "envify": "^3.2.0",
    "hat": "0.0.3",
    "octokat": "^0.4.5",
    "queue-async": "^1.0.7"
  },
  "devDependencies": {
    "dox": "^0.6.1",
    "browserify": "^8.1.3",
    "doxme": "^1.4.3",
    "jshint": "^2.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mapbox/hubdb.git"
  }
}
